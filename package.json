        "email": "me@arrayiterator.com",
        "url": "https://www.arrayiterator.com"
    },
    "scripts": {
        "clean:dist": "rm -rf dist",
        "clean:module": "rm -rf build",
        "clean:types": "rm -rf @types",
        "clean:all": "npm run clean:module && npm run clean:dist && npm run clean:types",
        "build:types": "npm run clean:types && tsc --declaration --declarationDir @types",
        "build:dist": "npm run clean:dist && webpack --mode=production --config webpack.config.js",
        "build:module": "npm run clean:module && tsc --outDir build",
        "build:all": "npm run build:module && npm run build:dist && npm run build:types"
    },
    "main": "build/gettext-l10n.js",
    "types": "@types/**/*.d.ts",
    "devDependencies": {
        "terser-webpack-plugin": "^5.3.10",
        "ts-loader": "^9.5.1",
        "typescript": "^5.6.3",
        "webpack": "^5.95.0",
        "webpack-cli": "^5.1.4"
    }
}